<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
     <!-- Multi - Company Rules -->
    <record id="school_student_comp_rule" model="ir.rule">
        <field name="name">School Student multi-company</field>
        <field name="model_id" ref="model_school_student"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

    <record id="school_exam_comp_rule" model="ir.rule">
        <field name="name">School Exam multi-company</field>
        <field name="model_id" ref="model_school_exam"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

    <record id="school_class_comp_rule" model="ir.rule">
        <field name="name">School Class multi-company</field>
        <field name="model_id" ref="model_school_class"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

    <record id="school_subject_comp_rule" model="ir.rule">
        <field name="name">School Subject multi-company</field>
        <field name="model_id" ref="model_school_subject"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>

    <record id="students_feedback_comp_rule" model="ir.rule">
        <field name="name">Students Feedback multi-company</field>
        <field name="model_id" ref="model_students_feedback"/>
        <field name="domain_force">[('company_id', 'in', company_ids)]</field>
    </record>



</odoo>
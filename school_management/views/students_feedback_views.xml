<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="action_student_feedback" model="ir.actions.client">
        <field name="name">Student Feedback</field>
        <field name="tag">student_feedback</field>
    </record>

    <record id="student_feedback_list" model="ir.ui.view">
        <field name="name">students.feedback.list</field>
        <field name="model">students.feedback</field>
        <field name="arch" type="xml">
            <list string="Student Feedback list">
                <field name="student_id" string="Student Name"/>
                <field name="class_id" string="Class"/>
                <field name="teacher_id" string="Teacher Name"/>
                <field name="rating" string=" Overall Rating"/>
                <field name="remarks" string="Remarks"/>
                <field name="discipline_grade" string="Discipline_Grade"/>
            </list>
        </field>
    </record>

     <record id="action_student_feedback_list" model="ir.actions.act_window">
        <field name="name">Student</field>
        <field name="res_model">students.feedback</field>
        <field name="view_mode">list</field>
    </record>
</odoo>